<%- include('partials/_header') %> <%- include('partials/_navbar') %>
<div class="container-fluid">
  <div class="entry-form row justify-content-center">
    <!--ADD ACTION-->
    <form method='POST'>
      <div class="form-group">
        <h1 class="register d-flex justify-content-center">REGISTER</h1>
        <label for="registerFirstName">First Name:</label>
        <input type="text" class="form-control" id="registerFirstName" name="first_name" />
      </div>
      <div class="form-group">
        <div class="form-group"></div>
          <label for="registerLastName">Last Name:</label>
          <input type="text" class="form-control" id="registerLastName" name="last_name" />
        </div>
        <div class="form-group">
          <label for="registerEmail">Email:</label>
          <input type="email" class="form-control" id="registerEmail" name="email" />
        </div>
        <div class="form-group">
        <div class="form-group">
        <label for="registerPassword">Password</label>
        <input
          type="password"
          class="form-control"
          id="registerPassword"
          name="password"
        />
      </div>
      <button type="submit" class="btn btn-primary">Register</button>
    </form>
  </div>
</div>
<%- include('partials/_footer') %>
